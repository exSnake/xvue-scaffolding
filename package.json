{
  "name": "xvue-scaffolding",
  "version": "1.0.0",
  "description": "Vue 3 scaffolding template with i18n, analytics, SEO, and email integration",
  "author": "{{AUTHOR_NAME}}",
  "license": "MIT",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "{{REPO_URL}}"
  },
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-pattern 'dist/**'",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-pattern 'dist/**'",
    "setup": "node scripts/setup.js",
    "release": "standard-version",
    "check-deps": "depcheck"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.956.0",
    "@iconify/vue": "^5.0.0",
    "@popperjs/core": "^2.11.8",
    "@vercel/analytics": "^1.5.0",
    "@vueuse/core": "^14.1.0",
    "flowbite": "^3.1.2",
    "lucide-vue-next": "^0.562.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-gtag": "^3.5.1",
    "vue-i18n": "^11.2.7",
    "vue-router": "^4.5.1",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@tailwindcss/forms": "^0.5.10",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "inquirer": "^9.0.0",
    "postcss": "^8.5.3",
    "prettier": "3.6.2",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.5",
    "vue-eslint-parser": "^10.1.4"
  }
}
